apt install python python3 python3.7 python-pip python3-pip -y ; curl "https://s3.amazonaws.com/aws-cli/awscli-bundle.zip" -o "awscli-bundle.zip" ; apt install unzip -y ;  unzip awscli-bundle.zip ; sudo ./awscli-bundle/install -i /usr/local/aws -b /usr/local/bin/aws ;  ./awscli-bundle/install -h ; apt install awscli -y ; echo "complete -C '/usr/bin/aws_completer' aws" >> ~/.bashrc ; source ~/.bashrc
